{% if request_ajax() %}
<style> 
section.hero.presentation  {
	background: url("data:image/svg+xml;utf8,{{svgbooks|replace({'<svg ': '<svg style=\"fill:#4a7680\" '})|url_encode}}");
	background-color: #385d66;
	background-repeat: no-repeat;
	background-attachment: fixed;
	background-size: contain; 
} 
</style>
{% endif %}  

<section class="hero presentation is-medium scroll-back">  
	<div class="hero-head "></div>		
	<div class="hero-body notification container">
	<div class="columns">
		<div class="column">
			  <figure class="image is-128x128 in-view" style="
    margin: auto;">
				<p>
					{% for images in page('/yogaenjoie/snippets/snippet-presentation').pages_yogaenjoie_images|slice(0,1) %} 
							<a href="#/presentation"> 
								{{ images.image().fit(140, 140).class("is-rounded").attr('loading', 'lazy')|raw }}  
							</a>    
					{% endfor %} 
				</p>
			  </figure>
		</div>
	
		  <div class="column in-view">
			<div> 
				{{ page('/yogaenjoie/snippets/snippet-presentation').content|raw}} 
				{% include "theme::partials/adminlink" with {'id': 36 } %} 
			</div> 
		  </div>
		</div> 
	 
	</div>
</section>
<p>{# publications #}</p>
<section class="hero publications is-large scroll-back"> 
	<div class="hero-head "></div>		 

		{% if request_ajax() %}
		<style> 
		section.hero.publications  {
			background: url("data:image/svg+xml;utf8,{{svgbooks|replace({'<svg ': '<svg style=\"fill:#5ba592\" '})|url_encode}}");
			background-color: #4e9986;
			background-repeat: no-repeat;
			background-attachment: fixed;
			background-size: contain;
		} 
		</style>
		{% endif %}  






	<div class="hero-body notification container">
		{# loop publications #}  
		{% set books = entries('pages', 'pages').where('parent_id', 17).get() %} 
		<div class="has-text-centered columns">			 
		{% for book in books|slice(1, 4) %} 
			{%if book.enabled %}      
							<div class="column">	 
									<div class="box in-view">
										{% for images in book.pages_yogaenjoie_images|slice(0,1) %}
											<div>
												<a href="#/publications"> 
													{{ images.image().fit(140, 100).attr('loading', 'lazy')|raw }}  
												</a>   
											</div>
										{% endfor %} 
										<p class="subtitle">
											<a href="#/publications">   
												{{ book.title|raw }}
											</a> 
										</p> 
									</div>
							</div> 	  
			{%endif%} 
		 {% endfor %} 
		</div> 
		
		{# BEWARE of the transformation to HTML of gt symbol below, use greaterthan plugin  #}
		{%if greaterthan(books|length, 4) %}     
			<div class="has-text-centered columns">			 
			{% for book in books|slice(5, books|length) %} 
				{%if book.enabled %}     
							<div class="column">
									<div class="box in-view">	 
										{% for images in book.pages_yogaenjoie_images|slice(0,1) %}
											<div>
												<a href="#/publications"> 
													{{ images.image().fit(140, 100).attr('loading', 'lazy')|raw }}  
												</a>   
											</div>
										{% endfor %} 
										<p class="subtitle">
											<a href="#/publications">   
												{{ book.title|raw }}
											</a> 
										</p> 
									</div>	 
							</div> 	  
				{%endif%} 
			 {% endfor %} 
			</div> 	  
		{%endif%} 
	</div> 
</section>

