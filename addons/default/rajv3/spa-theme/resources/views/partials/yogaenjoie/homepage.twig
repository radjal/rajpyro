{% if request_ajax() %}
	<style type="text/css"> 
			.column.box {
				margin: 0 10px 10px;
			}
			section.hero.home-3 .is-child.tile {
				padding: 10px;
			}  	
			section.home-3 p.subtitle.has-text-centered {
				padding-top: 4em;
				padding-bottom: 0.5em;
			}
			#home svg {
				width: 60%; 
				height: 60%; 
			}  
			svg.home-icon {
				width: 50%;
				margin-top: 80px;
				background-color: transparent;
				fill: #f5f5f5;
			}
			.snippet-home-svg {
				background-color: #ff9b28;
			}
			.hero-body {
				background: transparent;
			}
			section.hero.cours-stage {    
				background-color: #7f3db8e6; 
				color: #fff;
			}
			.hero .fa.fa-calendar-check-o {
				color: rebeccapurple;
			}
			.snippet-home-slides img {
				width: 100%;
			}
			#slides {
				background-color: #662f8a;
			}
			section.hero.home-1 {
				background-color: #ff9100;
				color: white;
			}
			.home-1 svg.home-icon {
				fill: white;
			}
			.home-1 strong {
				color: white;
			}
			.home-2 svg.home-icon {
				fill: white;

			}
			section.hero.home-2 {
				color: white;
				background-color: #ff9e00;
			}
			section .home-3 h3.title {
				margin-top: 5em;
				margin-bottom: 1em;
			} 
			section.hero.home-3 {
				background-color: #8c4eb5;
				color: #ffffff;
			}
			button.button.is-link.is-outlined {
				background-color: transparent;
				border-color: #dc8d32;
				color: #dc8d32;
			}
			.button.is-link.is-outlined:focus,
			.button.is-link.is-outlined:hover {
				background-color: transparent;
				border-color: #c75e09;
				color: #c75e09;
			}
			
			section.hero.presentation {
				background-color: #385d66;
				color: #fff;
			} 
			section.hero.publications {
				background-color: #4e9986;
				color: #fff;
			}
			section.publications p.subtitle a {
				text-decoration: none !important;
			}
			.publications div a img {
				width: 50%;
				min-width: 140px;
			}
			.publications div.column div:nth-child(1)  { 
				background-color: transparent;
				color: white;
				box-shadow: none;
			}  
			/* hide slide dots*/
			.dot {
				display:none !important;
			}
			/* responsive */
			@media screen and (min-width: 376px) { 
				#home svg {
					width: 80%; 
					height: 80%;
					/* min-width: 400px; */
					/* margin: auto; */
				}
			}
			@media screen and (min-width: 472px) { 
				#home svg {
					max-width: 50%;
					float: left;
				}
			}
			@media screen and (min-width: 769px) {  
			}
			@media screen and (min-width: 1024px) { 
			}
	</style>
{% endif %}

<section id="home" class="hero snippet-home-svg is-large">
	{{ page('/yogaenjoie/snippets/snippet-home-svg').content|raw}} 
</section>
{% include "theme::partials/adminlink" with {'id': 38 } %}  
<p>{# Cours et prochain stage... #} </p>
<section class="hero cours-stage is-medium scroll-back">
<div class="hero-head"></div>
	<div class="hero-body notification snippet-home-top"> 
		<div class="container"> 
			{# Snippet Home Top - Cours #} 
			{% for section in entries('pages', 'pages').where('id', 16).get() %} 
				{% if section.enabled %}    
					<p>			
						{{ section.content|raw }} 
					</p>			 
					<div>			
							{% for images in section.pages_yogaenjoie_images|slice(0,1) %}
								<a href="{{ request_root() }}/files/yogaenjoie/{{ images.name }}"> 
									{{ images.image().fit(100, 100).attr('loading', 'lazy')|raw   }}
									{{ section.title|raw }}
								</a>    
							{% endfor %}
					</div> 
				{% endif %} 
			 {% endfor %}
		</div> 
	</div>
	{% include "theme::partials/adminlink" with {'id': 16 } %}  
</section>
<section id="slides" class="hero snippet-home-slides is-medium">
	{{ page('/yogaenjoie/snippets/snippet-home-slides').content|raw}} 
	{# page('/yogaenjoie/snippets/snippet-home-slides').code|raw#} 
	{% include "theme::partials/adminlink" with {'id': 60 } %}  
</section>
